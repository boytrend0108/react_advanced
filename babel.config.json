{
  "presets": ["@babel/preset-env", "@babel/preset-typescript"],
  "plugins": [
    [
      "i18next-extract",
      {
        "nsSeparator": "~",
        "outputPath": "public/locales/{{locale}}/{{ns}}.json",
        "locales": ["en", "ua"],
        "ns": ["about", "main"],
        "defaultValue": "",
        "keyAsDefaultValue": true,
        "jsonIndent": 2,
        "useI18nextDefaultValue": true,
        "fallbackNS": "translation"
      }
    ]
  ]
}
