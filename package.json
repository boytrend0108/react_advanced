{
  "name": "reactadvanced",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "webpack serve  --env PORT=3001",
    "start:dev:server": "node  ./json-server/index.js",
    "lint:ts": "eslint \"**/*.{ts,tsx}\" --config ./config/eslint/eslint.config.mjs",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix --config ./config/eslint/eslint.config.mjs",
    "build:dev": "webpack --env MODE=development",
    "build:prod": "webpack --env MODE=production",
    "lint": "npx eslint --config ./config/eslint/eslint.config.mjs",
    "lint:fix": "npx eslint --fix --config ./config/eslint/eslint.config.mjs",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "test:ui:ok": "npx loki approve",
    "storybook": "storybook dev -p 6006 --config-dir config/storybook",
    "storybook:build": "storybook build --config-dir config/storybook",
    "test:ui:json": "node scripts/generate-visual-json-report.ts",
    "test:ui:html": "npx reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
